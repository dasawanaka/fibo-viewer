		<rdfs:comment>QName: fibo-loan-ln-ev:LoanPaidInFull</rdfs:comment>
		<rdfs:isDefinedBy rdf:resource="https://spec.edmcouncil.org/fibo/ontology/LOAN/LoanContracts/LoanEvents/"/>
